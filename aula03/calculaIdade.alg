algoritmo "CalculaIdade"

// ISSO É UM COMENTÁRIO E NÃO É EXECUTADO 23654652#@$%#$%$#%$
var

nome: caracter
anoNasc: inteiro
idade: inteiro


inicio

escreval("Digite seu nome completo:")
leia(nome)
escreval("Digite seu ano de nascimento:")
leia(anoNasc)

idade <- 2023 - anoNasc

escreval(nome," A sua idade é: ",idade," anos de idade")


fimalgoritmo
